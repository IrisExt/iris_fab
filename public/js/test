   MNT_DECH_ENS_MOIS    MONEY                null,
   MNT_INST_MAT         MONEY                null,
   MNT_BAT_TER          MONEY                null,
   MNT_PREST            MONEY                null,
   MNT_FRAIS_G          MONEY                null,
